                    "type": {"text": {"length": 255}}
                  },
                  "ExpirationDate": {
                    "alias": "exp_date",
                    "comment": "Expiration date for lot's product.",
                    "nullable": false,
                    "type": {"datetime": {}}
                  }
                },
                "index": {
                  "PK": {"type": "primary", "aliases": ["id"]}
                }
              },
              "Warehouse": {
                "alias": "wrhs",
                "comment": "Warehouse.",
                "attribute": {
                  "StockRef": {
                    "alias": "stock_ref",
                    "comment": "Reference to inventory stock.",
                    "nullable": false,
                    "type": {"integer": {"subtype": "smallint", "unsigned": true}}
