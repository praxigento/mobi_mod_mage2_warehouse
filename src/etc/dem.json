                    "own": {"aliases": ["stock_ref"]},
                    "reference": {
                      "entity": {"completeAlias": "cataloginventory_stock"},
                      "aliases": ["stock_id"]
                    },
                    "action": {"delete": "restrict", "update": "restrict"}
                  }
                }
