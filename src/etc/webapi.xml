<?xml version="1.0"?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">

    <route url="/V1/prxgt/lot/read/" method="GET">
        <service class="Praxigento\Warehouse\Api\LotInterface" method="read"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>

    <route url="/V1/prxgt/warehouse/read/" method="GET">
        <service class="Praxigento\Warehouse\Api\WarehouseInterface" method="read"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>
</routes>
